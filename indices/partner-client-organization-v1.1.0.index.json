{
  "settings": {
    "number_of_shards": 3,
    "number_of_replicas": 1
  },
  "mappings": {
    "_meta": {
      "version": "1.1.0"
    },
    "_routing": {
      "required": true
    },
    "dynamic": "false",
    "properties": {
      "id": {
        "type": "keyword"
      },
      "mspId": {
        "type": "keyword"
      },
      "name": {
        "type": "text"
      },
      "email": {
        "type": "keyword"
      },
      "status": {
        "type": "keyword"
      },
      "countryCode": {
        "type": "keyword"
      },
      "created": {
        "type": "date"
      },
      "lastUpdated": {
        "type": "date"
      },
      "office365": {
        "properties": {
          "authState": {
            "type": "keyword"
          },
          "lastProcessed": {
            "type": "date"
          },
          "totalActivePaidUsers": {
            "type": "integer"
          },
          "totalLicenses": {
            "type": "integer"
          },
          "availableLicenses": {
            "type": "integer"
          },
          "totalSpend": {
            "type": "scaled_float",
            "scaling_factor": 100
          },
          "processFailed": {
            "type": "date"
          }
        }
      }
    }
  }
}
